{
  "name": "me",
  "author": {
    "name": "Natalia Pospieszy≈Ñska"
  },
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "gh-pages": "npm run build && npx gh-pages --dist build --message 'update gh-pages' --dotfiles",
    "gh-pages-help": "npx gh-pages --help",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "preview": "vite preview",
    "generatePdf": "npx tsx ./scripts/generatePdf.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.30.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "gh-pages": "^6.1.1",
    "pdf-lib": "^1.17.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "puppeteer": "^21.11.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tslib": "^2.7.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^4.5.3"
  },
  "dependencies": {
    "@picocss/pico": "^2.0.6",
    "lucide-svelte": "^0.435.0"
  }
}
