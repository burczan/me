{
  "name": "me",
  "author": {
    "name": "Natalia Pospieszy≈Ñska"
  },
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "gh-pages": "npm run build && npx gh-pages --dist build --message 'update gh-pages' --dotfiles",
    "gh-pages-help": "npx gh-pages --help",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "preview": "vite preview",
    "generatePdf": "npx tsx ./scripts/generatePdf.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "gh-pages": "^6.0.0",
    "pdf-lib": "^1.17.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "puppeteer": "^21.4.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "tsx": "^3.14.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  }
}
